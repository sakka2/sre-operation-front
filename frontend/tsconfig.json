{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "types": ["node"],
    "lib": ["es2019", "dom"],
    "allowJs": true, // jsコンパイル用
    "checkJs": true, // js簡易型チェック
    "outDir": "./dist/",
    "sourceMap": true,
    "inlineSources": true,
    "strict": false,
    "noImplicitAny": false,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
